{
  "name": "mgmt-v3",
  "version": "3.0.0",
  "private": true,
  "packageManager": "pnpm@10.23.0",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:safe": "./scripts/dev.sh start",
    "dev:bg": "./scripts/dev.sh bg",
    "dev:stop": "./scripts/dev.sh stop",
    "dev:status": "./scripts/dev.sh status",
    "dev:cleanup": "./scripts/dev.sh cleanup",
    "lint": "turbo lint",
    "test": "turbo test",
    "clean": "turbo clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },

  "devDependencies": {
    "@types/pdfkit": "^0.17.4",
    "prettier": "^3.2.0",
    "ts-node": "^10.9.2",
    "turbo": "^2.3.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=20.10.0"
  }
}
